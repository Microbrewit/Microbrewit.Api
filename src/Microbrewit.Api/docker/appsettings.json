{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Verbose",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
   "DatabaseSettings": {
      "DbConnection": "Server=postgres;Port=5432;Database=${POSTGRES_DB};User Id=${POSTGRES_USER};Password=${POSTGRES_PASSWORD};"

  },
  "ElasticSearch": {
    "Url": "http://elasticsearch:9200",
    "Index": "${ELASTICSEARCH_INDEX}"
  },
  "Api": {
    "Url": "${MICROBREWIT_APIURL}"
  }
}
