{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Verbose",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
   "DatabaseSettings": {
      "DbConnection": "Server=postgresql;Port=5432;Database=${POSTGRESQL_DB};User Id=${POSTGRESQL_USER};Password=${POSTGRESQL_PASSWORD};"

  },
  "ElasticSearch": {
      "Url": "elasticsearch:9200",
      "Index": "${ELASTICSEARCH_INDEX}"
  }
}
